set -x

impc_info()
{
    PREDEV=snd-hda-codec-conexant
    SNDDEV=snd-hda-intel model=ideapad
    CAMDEV=uvcvideo
    #modprobe v4l1_compat
    #enable cando touch screen driver
    modprobe hid_multitouch
    #enable 80211 WiFi
    modprobe lib80211
    #modprobe wl
    modprobe wmi
    #modprobe lsrot.ko
    echo WAKE_LOCK_SUSPEND > /sys/power/wake_lock
    return 1
}

detect_hardware()
{
	impc_info
}
